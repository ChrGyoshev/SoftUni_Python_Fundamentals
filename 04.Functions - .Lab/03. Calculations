operator = input()
first_num = int(input())
second_num = int(input())


def function(a,b,oper):

    if oper == "multiply":
        result = a*b
    elif oper == "divide":
        result = a//b
    elif oper== "add":
        result = a+b
    elif oper == "subtract":
        result = a-b
    return result



print(function(first_num,second_num,operator))
